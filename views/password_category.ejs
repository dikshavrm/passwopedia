<%- include('header') %>

      <!-- <h1>Logged In as: <% if(loginUser){ %><%= loginUser %><% } %></h1> -->
      <div class="container-fluid mt-5">

        <div class="row">
          <div class="col-md-2">
      
          </div>
          <div class="col-md-8">
            <div class="text-center dash1">Password category List</div>
            <div class="dash2">
      

    <table class="table table-striped">
        <thead>
            <th>Password Category Name</th>
            <th>Edit</th>
            <th>Delete</th>

        </thead>
        <tbody>
          <% if(records.length>0){
            records.forEach(function(row){
              %>
         
            <tr>
                <td><%= row.password_category %></td>
                <td><a href="/passwordCategory/edit/<%= row._id %>"><button class="btn btn-success">Edit</button></a></td>
                <td><a href="/passwordCategory/delete/<%= row._id %>"><button class="btn btn-danger">Delete</button></a></td>

            </tr>
            <%    })
          } %>
        </tbody>
    </table>

  </div>
</div>
<div class="col-md-2">
      
</div>
</div>
</div>
